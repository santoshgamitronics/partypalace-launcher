{
  "name": "partypalace-launcher",
  "version": "1.0.0",
  "description": "A windows/mac launcher for partypalace",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/gami-256x256.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/gami-256x256.png.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Partypalace launcher\""
  },
  "author": "Santosh Kumar M",
  "license": "ISC",
  "devDependencies": {
    "electron": "^15.3.0",
    "electron-packager": "^15.2.0",
    "nodemon": "^2.0.6"
  },
  "build": {
    "appId": "oikonomopo.electron-deeplinking-macos-win32",
    "protocols": {
      "name": "electron-deep-linking",
      "schemes": [
        "myapp"
      ]
    },
    "mac": {
      "category": "public.app-category.Reference"
    },
    "win": {}
  },
  "dependencies": {
    "electron-dl": "^3.3.0",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.3.9",
    "update-electron-app": "^2.0.1"
  }
}
